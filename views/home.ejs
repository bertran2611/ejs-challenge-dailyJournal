<%- include('partials/header') %> 

<h1>Home</h1>
<p><%= homeStartingContent %></p>


<% postsVar.forEach(pV => { %>
        <h1><%= pV.title %> </h1>
        <p style="overflow: hidden; text-overflow: ellipsis; display: -webkit-box; -webkit-line-clamp: 2; -webkit-box-orient: vertical;"><%= pV.content %></p>
        <a href="posts/<%= pV.title %> ">Continue Reading...</a>
    
<% }) %>


<%- include('partials/footer') %> 